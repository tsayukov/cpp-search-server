    },
    {
      "name": "release-dev-23",
      "configurePreset": "release-dev-23"
    },
    {
      "name": "release-dev-26",
      "configurePreset": "release-dev-26"
    },
    {
      "name": "docs",
      "inherits": "release-dev",
      "displayName": "Build the documentation",
      "targets": "search_server_docs"
    },
    {
      "name": "install",
      "inherits": "release",
      "displayName": "Install the project",
      "targets": "install"
    },
    {
      "name": "install-static-library",
      "configurePreset": "release-static-library",
      "displayName": "Install the project as a static library",
      "targets": "install"
    },
    {
      "name": "install-shared-library",
      "configurePreset": "release-shared-library",
      "displayName": "Install the project as a shared library",
      "targets": "install"
    },
    {
      "name": "install-configs",
      "inherits": "release",
      "displayName": "Install CMake configuration files",
      "targets": "install_search_server_configs"
    },
    {
      "name": "install-headers",
      "inherits": "release",
      "displayName": "Install headers",
      "targets": "install_search_server_headers"
    },
    {
      "name": "install-runtime",
      "inherits": "release",
      "displayName": "Install the runtime component",
      "targets": "install_search_server_runtime"
    },
    {
      "name": "install-development",
      "inherits": "release",
      "displayName": "Install the development component",
      "targets": "install_search_server_development"
    },
    {
      "name": "install-docs",
      "inherits": "release",
      "displayName": "Install the documentation",
      "targets": "install_search_server_docs"
    }
  ],
  "testPresets": [
